{
  "name": "telefonnummer",
  "version": "2.0.0",
  "description": "Phone number formatter for Swedish phone numbers",
  "main": "dist/telefonnummer.js",
  "module": "dist/telefonnummer.es6.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development tsc-watch --project tsconfig.json  --onSuccess \"rollup -c\"",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "test": "jest",
    "test:dev": "jest --watch",
    "coverage": "npm test -- --coverage",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && rollup -c && rimraf compiled",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/believer/telefonnummer.git"
  },
  "keywords": [
    "Phone",
    "Number",
    "Parser",
    "Swedish",
    "Telefonnummer",
    "Sverige"
  ],
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/telefonnummer/issues"
  },
  "homepage": "https://github.com/believer/telefonnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "@types/jest": "23.0.0",
    "@types/node": "10.1.4",
    "all-contributors-cli": "5.0.0",
    "cross-env": "5.1.6",
    "husky": "0.14.3",
    "jest": "23.1.0",
    "lint-staged": "7.1.3",
    "prettier": "1.13.5",
    "rimraf": "2.6.2",
    "rollup": "0.60.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-filesize": "2.0.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "4.0.0",
    "semantic-release": "15.5.1",
    "travis-deploy-once": "4.4.1",
    "ts-jest": "22.4.2",
    "tsc-watch": "1.0.22",
    "tslint": "5.9.1",
    "typescript": "2.9.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
