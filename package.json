{
  "name": "telefonnummer",
  "version": "2.0.0",
  "description": "Phone number formatter for Swedish phone numbers",
  "main": "dist/telefonnummer.js",
  "module": "dist/telefonnummer.es6.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development tsc-watch --project tsconfig.json  --onSuccess \"rollup -c\"",
    "lint": "eslint 'lib/**/*.{ts,tsx}'",
    "test": "is-ci \"test:ci\" \"test:watch\"",
    "test:ci": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && rollup -c && rimraf compiled",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/believer/telefonnummer.git"
  },
  "keywords": [
    "Phone",
    "Number",
    "Parser",
    "Swedish",
    "Telefonnummer",
    "Sverige"
  ],
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/telefonnummer/issues"
  },
  "homepage": "https://github.com/believer/telefonnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/jest": "26.0.15",
    "@types/jest-in-case": "1.0.3",
    "@types/node": "14.14.6",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "all-contributors-cli": "6.19.0",
    "cross-env": "7.0.2",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "is-ci-cli": "2.1.2",
    "jest": "26.6.1",
    "jest-in-case": "1.0.2",
    "lint-staged": "10.5.0",
    "prettier": "2.1.2",
    "rimraf": "3.0.2",
    "rollup": "2.32.1",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "9.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-uglify": "6.0.4",
    "semantic-release": "17.2.2",
    "tsc-watch": "4.2.9",
    "typescript": "4.0.5"
  }
}
