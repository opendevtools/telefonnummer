{
  "name": "telefonnummer",
  "version": "1.1.1",
  "description": "Phone number formatter for Swedish phone numbers",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "tsc -w",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "lint:js": "eslint index.js ./lib/**/*.js ./test/**/*.js",
    "test": "jest",
    "test:dev": "jest --watch",
    "coverage": "npm test -- --coverage",
    "build": "tsc --declaration",
    "prepare": "npm test && npm run lint && npm run build",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/believer/telefonnummer.git"
  },
  "keywords": [
    "Phone",
    "Number",
    "Parser",
    "Swedish",
    "Telefonnummer",
    "Sverige"
  ],
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/telefonnummer/issues"
  },
  "homepage": "https://github.com/believer/telefonnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "22.2.2",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-preset-env": "1.6.1",
    "eslint": "4.16.0",
    "husky": "0.14.3",
    "jest": "22.1.4",
    "lint-staged": "7.0.0",
    "prettier": "1.11.1",
    "ts-jest": "22.4.2",
    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
