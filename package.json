{
  "name": "telefonnummer",
  "version": "2.0.0",
  "description": "Phone number formatter for Swedish phone numbers",
  "main": "dist/telefonnummer.js",
  "module": "dist/telefonnummer.es6.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development tsc-watch --project tsconfig.json  --onSuccess \"rollup -c\"",
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "test": "jest",
    "test:dev": "jest --watch",
    "coverage": "npm test -- --coverage",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json && rollup -c && rimraf compiled",
    "prepare": "npm run build",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/believer/telefonnummer.git"
  },
  "keywords": [
    "Phone",
    "Number",
    "Parser",
    "Swedish",
    "Telefonnummer",
    "Sverige"
  ],
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/telefonnummer/issues"
  },
  "homepage": "https://github.com/believer/telefonnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "@types/jest": "23.3.12",
    "@types/node": "10.12.18",
    "all-contributors-cli": "5.6.0",
    "cross-env": "5.2.0",
    "husky": "1.3.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "rimraf": "2.6.3",
    "rollup": "1.0.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-filesize": "5.0.1",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "6.0.0",
    "semantic-release": "15.13.2",
    "travis-deploy-once": "5.0.11",
    "ts-jest": "23.10.5",
    "tsc-watch": "1.0.31",
    "tslint": "5.12.0",
    "typescript": "3.2.2"
  }
}
