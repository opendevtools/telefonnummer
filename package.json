{
  "name": "telefonnummer",
  "version": "1.0.3",
  "description": "Phone number formatter for Swedish phone numbers",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'lib/**/*.{ts,tsx}'",
    "lint:js": "eslint index.js ./lib/**/*.js ./test/**/*.js",
    "test": "jest",
    "test:dev": "jest --watch",
    "coverage": "npm test -- --coverage",
    "build": "tsc",
    "prepare": "npm test && npm run lint && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/believer/telefonnummer.git"
  },
  "keywords": [
    "Phone",
    "Number",
    "Parser",
    "Swedish",
    "Telefonnummer",
    "Sverige"
  ],
  "author": {
    "name": "Rickard Laurin",
    "email": "rickard.laurin@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/believer/telefonnummer/issues"
  },
  "homepage": "https://github.com/believer/telefonnummer#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "22.1.1",
    "babel-eslint": "8.2.1",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "4.16.0",
    "jest": "22.1.4",
    "ts-jest": "22.0.2",
    "tslint": "^5.9.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  }
}
